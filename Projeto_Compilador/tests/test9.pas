program ExemploVariaveis;

var
  nome: string;
  idade: integer;
  altura: real;

begin
  { Atribuindo valores às variáveis }
  nome := 'Ana';
  idade := 25;
  altura:= 1.70;


  { Exibindo os valores das variáveis }
  writeln('Nome: ', nome);
  writeln('Idade: ', idade);
  writeln('Altura:', altura);

end.
